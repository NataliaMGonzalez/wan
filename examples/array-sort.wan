program bubblesort;

int i, j, aux;

int a[6];

a[0] = 8;
a[1] = 3;
a[2] = 10;
a[3] = 8;
a[4] = 4;
a[5] = 1;

i = 0;

# Bubble sort
while(i < 6){
    j = 0;
    while(j < 6 - 1){
        if(a[j] > a[j + 1]){
            aux = a[j];
            a[j] = a [j + 1];
            a[j + 1] = aux; 
        }
        j = j + 1;
    }
    i = i + 1;
}

# print array
i = 0;
print("Bubble sort");
while(i < 6){
    print(a[i]);
    i = i + 1;
}