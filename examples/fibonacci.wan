int fibonacciRecursive(int n){
    if(n == 0){
        return 0;
    }
    if(n == 1){
        return 1;
    }
    return fibonacciRecursive(n -1) + fibonacciRecursive(n - 2);
}

int fibonacciIterative(int n){
    int a, b, c, i;
    a = 0;
    b = 1;
    i = 2;

    if (n == 0){
        return 0;
    }

    while(i < n){
        c = a + b;
        a = b;
        b = c;

        i = i + 1;
    }
    return b;
}


int a;

a = fibonacciRecursive(10);

print(a);